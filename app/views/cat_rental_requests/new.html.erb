<title> Create A New Request! </title>

<form action="/cat_rental_requests" method="post">

  <label for="sd">Start Date</label>
  <input type="date" id="sd" name="cat_rental_request[start_date]">
  <br>

  <label for="ed">End Date</label>
  <input type="date" id="ed" name="cat_rental_request[end_date]">
  <br>

  <label for="cat_id">Cat id </label>
  <select name="cat_rental_request[cat_id]" id="cat_id">
    <% @cats.each do |cat| %>
      <option value=<%= cat.id %>><%= cat.name %></option>
    <% end %>
  </select>

  <input type="submit" value="Make Request">

</form>
